<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XFLoadFromXaml.Pages.MyDynamicView">
    <StackLayout>
        <Label Text="{Binding MyTitle}" VerticalOptions="Center" HorizontalOptions="Center" />
        <Entry Text="{Binding MyValue}" VerticalOptions="Center" HorizontalOptions="Center" />
    </StackLayout>
</ContentView>